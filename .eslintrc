{
  "extends": ["plugin:vue/essential", "standard"],
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "allowImportExportEverywhere": true,
    "generator-star-spacing": "off",
    "quotes": 0,
    "space-before-function-paren": ["error", "never"],
    "semi": 0
  }
}
